<?xml version="1.0" encoding="UTF-8"?>
<questions>
    <question id="goals">
        <text>Unternehmensziele (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="brand_awareness">Markenbekanntheit steigern</option>
            <option id="sales">Verkäufe fördern</option>
            <option id="customer_loyalty">Kundenbindung erhöhen</option>
            <option id="lead_generation">Lead-Generierung verbessern</option>
            <option id="customer_service">Kundenservice verbessern</option>
        </options>
    </question>
    <question id="target_audience">
        <text>Zielgruppe (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="teens">Jugendliche (13-17 Jahre)</option>
            <option id="young_adults">Junge Erwachsene (18-24 Jahre)</option>
            <option id="adults">Erwachsene (25-34 Jahre)</option>
            <option id="middle_aged">Mittleres Alter (35-54 Jahre)</option>
            <option id="older_adults">Ältere Erwachsene (55+ Jahre)</option>
        </options>
    </question>
    <question id="industry_competition">
        <text>Branche und Wettbewerb (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="high">Sehr wettbewerbsintensiv</option>
            <option id="moderate">Mäßig wettbewerbsintensiv</option>
            <option id="low">Wenig wettbewerbsintensiv</option>
        </options>
    </question>
    <question id="previous_activities">
        <text>Bisherige Aktivitäten (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="facebook">Facebook</option>
            <option id="instagram">Instagram</option>
            <option id="twitter">Twitter</option>
            <option id="linkedin">LinkedIn</option>
            <option id="tiktok">TikTok</option>
            <option id="youtube">YouTube</option>
        </options>
    </question>
    <question id="content_preferences">
        <text>Content-Vorlieben (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="videos">Videos</option>
            <option id="articles">Artikel</option>
            <option id="images">Bilder</option>
            <option id="tutorials">Tutorials</option>
            <option id="infographics">Infografiken</option>
            <option id="podcasts">Podcasts</option>
        </options>
    </question>
    <question id="resources">
        <text>Ressourcen (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="under_500">Unter $500 pro Monat</option>
            <option id="500_1000">$500 bis $1.000 pro Monat</option>
            <option id="1000_5000">$1.000 bis $5.000 pro Monat</option>
            <option id="over_5000">Über $5.000 pro Monat</option>
        </options>
    </question>
    <question id="time_commitment">
        <text>Zeitlicher eigener Einsatz pro Woche (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="less_5">Weniger als 5 Stunden</option>
            <option id="5_10">5 bis 10 Stunden</option>
            <option id="10_20">10 bis 20 Stunden</option>
            <option id="over_20">Mehr als 20 Stunden</option>
        </options>
    </question>
    <question id="customer_interaction">
        <text>Kundeninteraktion (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="very_active">Sehr aktiv</option>
            <option id="moderate_active">Moderat aktiv</option>
            <option id="less_active">Wenig aktiv</option>
            <option id="none">Nicht vorhanden</option>
        </options>
    </question>
    <question id="sales_channels">
        <text>Verkaufskanäle (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="direct_sales">Direkter Verkauf</option>
            <option id="lead_generation">Lead-Generierung</option>
            <option id="both">Beides</option>
            <option id="none">Keines von beidem</option>
        </options>
    </question>
    <question id="customer_feedback">
        <text>Kundenfeedback (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="direct_social_media">Direktes Feedback über Social Media</option>
            <option id="surveys">Umfragen</option>
            <option id="review_platforms">Bewertungsplattformen</option>
            <option id="personal_feedback">Persönliches Feedback</option>
            <option id="no_systematic">Keine systematische Erfassung</option>
        </options>
    </question>
    <question id="crisis_management">
        <text>Krisenmanagement (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="experienced">Erfahren im Umgang mit Krisen</option>
            <option id="some_experience">Einige Erfahrungen</option>
            <option id="no_experience">Keine Erfahrungen</option>
        </options>
    </question>
    <question id="brand_consistency">
        <text>Markenkonsistenz (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="very_important">Sehr wichtig</option>
            <option id="important">Wichtig</option>
            <option id="less_important">Weniger wichtig</option>
            <option id="unimportant">Unwichtig</option>
        </options>
    </question>
    <question id="tone">
        <text>Sprache und Ton (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="formal">Formal</option>
            <option id="informal">Informell</option>
            <option id="humorous">Humorvoll</option>
            <option id="serious">Ernst</option>
            <option id="inspiring">Inspirierend</option>
        </options>
    </question>
    <question id="influencer_involvement">
        <text>Einbindung von Influencern (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="regular">Ja, regelmäßig</option>
            <option id="occasional">Gelegentlich</option>
            <option id="planned">Nein, aber geplant</option>
            <option id="not_planned">Nein, nicht geplant</option>
        </options>
    </question>
    <question id="campaigns">
        <text>Werbeaktionen und Kampagnen (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="discounts">Rabatte und Sonderangebote</option>
            <option id="contests">Gewinnspiele</option>
            <option id="product_demos">Produktdemonstrationen</option>
            <option id="partnerships">Partnerschaften</option>
            <option id="none">Keine</option>
        </options>
    </question>
    <question id="seasonality">
        <text>Saisonabhängigkeit (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="very_strong">Ja, sehr stark</option>
            <option id="manageable">Ja, aber überschaubar</option>
            <option id="none">Nein</option>
        </options>
    </question>
    <question id="compliance">
        <text>Datenschutz und Compliance (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="high">Hohe Anforderungen</option>
            <option id="moderate">Moderate Anforderungen</option>
            <option id="low">Geringe Anforderungen</option>
        </options>
    </question>
    <question id="tools">
        <text>Technologie und Tools (Mehrfachauswahl)</text>
        <type>checkbox</type>
        <options>
            <option id="social_media_management">Social-Media-Management-Tools</option>
            <option id="analytics_tools">Analytik-Tools</option>
            <option id="automation_tools">Automatisierungs-Tools</option>
            <option id="no_special_tools">Keine speziellen Tools</option>
        </options>
    </question>
    <question id="training">
        <text>Ausbildung und Fortbildung (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="regular">Regelmäßige Schulungen</option>
            <option id="occasional">Gelegentliche Schulungen</option>
            <option id="none">Keine Schulungen</option>
        </options>
    </question>
    <question id="future_vision">
        <text>Zukunftsvision (Einfachauswahl)</text>
        <type>radio</type>
        <options>
            <option id="expansion">Starke Expansion auf neuen Plattformen</option>
            <option id="deepening_presence">Vertiefung der bestehenden Präsenz</option>
            <option id="experimenting_formats">Experimentieren mit neuen Formaten</option>
            <option id="stabilization">Stabilisierung der aktuellen Situation</option>
        </options>
    </questions>
</questions>
